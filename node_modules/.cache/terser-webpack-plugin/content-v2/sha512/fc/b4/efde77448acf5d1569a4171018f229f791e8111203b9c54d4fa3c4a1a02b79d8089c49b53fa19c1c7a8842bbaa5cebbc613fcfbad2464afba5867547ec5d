Quantity && touched.currentQuantity ?\\r\\n                                    <StyledInputError\\r\\n                                        onChange={this.handleInputChange}\\r\\n                                        name=\\\"currentQuantity\\\"\\r\\n                                        type=\\\"number\\\"\\r\\n                                        onBlur={handleBlur}\\r\\n                                        value={values.currentQuantity}\\r\\n                                        placeholder=\\\"\\\"/>\\r\\n                                    :\\r\\n                                    <StyledInput\\r\\n                                        onChange={this.handleInputChange}\\r\\n                                        name=\\\"currentQuantity\\\"\\r\\n                                        type=\\\"number\\\"\\r\\n                                        onBlur={handleBlur}\\r\\n                                        value={values.currentQuantity}\\r\\n                                        placeholder=\\\"\\\"/>\\r\\n                                }\\r\\n                            </FormItem>\\r\\n                            {errors.currentQuantity && touched.currentQuantity ?\\r\\n                                <ErrorText>{errors.currentQuantity}</ErrorText> : null\\r\\n                            }\\r\\n\\r\\n                            <ButtonContainer>\\r\\n                                <Link to=\\\"/register\\\">\\r\\n                                    <ButtonIcon\\r\\n                                        icon={decline}\\r\\n                                    />\\r\\n                                </Link>\\r\\n                                {isValid ?\\r\\n                                    <ButtonIcon\\r\\n                                        onClick={ () => errors ? this.notify(this.state.name) : null}\\r\\n                                        type=\\\"submit\\\"\\r\\n                                        icon={accept}